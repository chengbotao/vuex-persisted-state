{
  "name": "vuex-persisted-states",
  "version": "1.0.10",
  "description": "Keep your vuex state between page reloads.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": "git@github.com:chengbotao/vuex-persisted-states.git",
  "author": "chengbotao <chengbotao5221@163.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuex",
    "storage",
    "ts",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-ts",
    "build-ts": "npx tsc -p tsconfig.json",
    "clean": "rimraf ./dist",
    "commit": "npx git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "cz-customizable": "^6.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
